<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:height="80dp"
                android:width="80dp"/>
            <solid android:color="@color/kh_light"/>
        </shape>
    </item>
    <item android:top="30dp">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/deep_purple500"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent"/>
            <stroke
                android:color="@color/grey600"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>


